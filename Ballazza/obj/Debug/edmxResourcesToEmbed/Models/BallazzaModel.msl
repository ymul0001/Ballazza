      </EntityTypeMapping>
    </EntitySetMapping>
    <EntitySetMapping Name="Venues">
      <EntityTypeMapping TypeName="Model.Venue">
        <MappingFragment StoreEntitySet="Venue">
          <ScalarProperty Name="VenueId" ColumnName="VenueId" />
          <ScalarProperty Name="VenueName" ColumnName="VenueName" />
          <ScalarProperty Name="VenueStreet" ColumnName="VenueStreet" />
          <ScalarProperty Name="VenueSuburb" ColumnName="VenueSuburb" />
          <ScalarProperty Name="VenueState" ColumnName="VenueState" />
          <ScalarProperty Name="VenuePostcode" ColumnName="VenuePostcode" />
          <ScalarProperty Name="VenuePhoneno" ColumnName="VenuePhoneno" />
